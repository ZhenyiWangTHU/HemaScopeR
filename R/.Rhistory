folder_path <-'D:/GitHub/HemaScopeR/R'
# 列出所有 R 文件
files <- list.files(path = folder_path, pattern = "\\.R$", full.names = TRUE)
# 获取文件名（不包括路径）
file_names <- basename(files)
# 按字母顺序排序
sorted_files <- sort(file_names)
# 将文件名整理成 Collate 格式
collate_format <- paste(sorted_files, collapse = ", ")
# 输出结果
cat(collate_format)
